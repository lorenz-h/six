{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters":{
        "colored":{
            "class": "utils.logging._ColoredFormatter",
            "format": "%(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers":{
        "console":{
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "colored",
            "stream": "ext://sys.stdout"
        }
    },
    "root":{
        "level":"INFO",
        "handlers":["console"]
    }
}
  